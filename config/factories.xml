		<class default="no" id="Php_AndreaBoccaccio_Db_DbVerySimpleMySql" />
	</factory>
	<factory id="login">
		<class default="yes" id="Php_AndreaBoccaccio_Login_LoginNull" />
		<class default="no" id="Php_AndreaBoccaccio_Login_LoginVoid" />
		<class default="no" id="Php_AndreaBoccaccio_Login_LoginRandom" />
	</factory>
	<factory id="view">
		<class default="yes" id="Php_AndreaBoccaccio_View_ViewNull" />
		<class default="no" id="Php_AndreaBoccaccio_View_ViewVoid" />
