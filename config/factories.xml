	<factory id="mappingModel">
		<class default="yes" id="Php_AndreaBoccaccio_Model_MappingModelNull" />
		<class default="no" id="Php_AndreaBoccaccio_Model_MappingModelVoid" />
		<class default="no" id="Php_AndreaBoccaccio_Model_MappingModelCause" />
	</factory>
</factories>